{
  "name": "ts-template",
  "version": "0.0.0",
  "license": "MIT",
  "author": "SerKo (http://github.com/serkodev)",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/serkodev/ts-template#readme",
  "bugs": {
    "url": "https://github.com/serkodev/ts-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serkodev/ts-template.git"
  },
  "main": "./dist/index.global.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "lint": "eslint .",
    "start": "tsx ./src/index.ts",
    "dev": "tsx watch ./src/index.ts",
    "prepublishOnly": "pnpm build",
    "release": "pnpm lint && bump --commit --push --tag && npm publish"
  },
  "packageManager": "pnpm@7.0.0",
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@types/node": "^17.0.35",
    "eslint": "^8.15.0",
    "tsx": "^3.3.1",
    "typescript": "^4.6.4",
    "tsup": "^5.12.8",
    "version-bump-prompt": "^6.1.0"
  }
}
